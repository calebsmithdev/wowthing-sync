{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run generate",
    "devPath": "http://localhost:3000",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Wowthing Sync",
    "version": "0.2.1"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "allowlist": {
      "all": false,
      "path": {
        "all": true
      },
      "os": {
        "all": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": false,
        "open": true
      },
      "http": {
        "all": true,
        "request": true
      },
      "fs": {
        "all": false,
        "scope": ["**"],
        "readFile": true,
        "readDir": true,
        "exists": true
      },
      "process": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "Productivity",
      "copyright": "2024 Caleb Smith",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "wowthing.sync",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
          "https://github.com/calebsmithdev/wowthing-sync/releases/latest/download/latest.json"
      ],
      "dialog": false,
      "windows": {
        "installMode": "passive"
      },
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDZFNjQ4NTAxNjlDNjAzOEMKUldTTUE4WnBBWVZrYnR0ZTNsdEdudVdQNlVpS1hibXZoVzRBYXZYYU02ZVZjNHpSaXNjVHZnMVAK"
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 500,
        "width": 500,
        "resizable": false,
        "title": "WoWthing Sync",
        "decorations": true
      }
    ]
  }
}
