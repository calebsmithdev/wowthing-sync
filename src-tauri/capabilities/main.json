{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main",
  "description": "Capability for the main window",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "store:default",
    "process:default",
    "updater:default",
    "fs:default",
    {
      "identifier": "fs:allow-exists",
      "allow": [{ "path": "$APPDATA/*" }]
    }
  ]
}
